<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:TerasoiogyLauncher.ViewModels"
                    xmlns:view="clr-namespace:TerasoiogyLauncher.Views">
    <DataTemplate DataType="{x:Type vm:HomeViewModel}">
        <view:HomeView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:DownloadGameViewModel}">
        <view:DownloadGameView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:DevTestViewModel}">
        <view:DevTestView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:DevControlsViewModel}">
        <view:DevControlsView />
    </DataTemplate>
</ResourceDictionary>